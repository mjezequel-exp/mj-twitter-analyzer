# Environment variables for development
# Copy this file to .env.development.local and fill with real values

VITE_APP_ENV=Development

# Azure Application Insights
VITE_APP_APPINSIGHTS_CONNECTION_STRING=InstrumentationKey=YOUR_APPINSIGHTS_KEY;IngestionEndpoint=https://northeurope-2.in.applicationinsights.azure.com/;LiveEndpoint=https://northeurope.livediagnostics.monitor.azure.com/;ApplicationId=YOUR_APP_ID

# Azure AD Configuration  
VITE_APP_AZURE_AD_CLIENTID=YOUR_AZURE_AD_CLIENT_ID
VITE_APP_AZURE_AD_TENANTID=YOUR_AZURE_AD_TENANT_ID
VITE_APP_AZURE_AD_SCOPES=openid profile
VITE_APP_AZURE_AD_INSTANCE=https://login.microsoftonline.com/
VITE_APP_AZURE_AD_CALLBACKPATH=/redirect

# Azure OpenAI Configuration
VITE_AZURE_OPENAI_ENDPOINT=https://your-azure-openai-instance.openai.azure.com/
VITE_AZURE_OPENAI_KEY=YOUR_AZURE_OPENAI_API_KEY
VITE_AZURE_OPENAI_DEPLOYMENT=gpt-4o
VITE_AZURE_OPENAI_VERSION=2025-01-01-preview

# Web API (if needed)
VITE_APP_WEB_API_BASE_URL=https://your-api.azurewebsites.net/